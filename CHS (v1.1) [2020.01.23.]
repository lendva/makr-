    ActiveWindow.SmallScroll Down:=9
    With ActiveSheet.QueryTables.Add(Connection:= _
        "URL;https://www.informateka.hu/_hydra/admin-partner-termekek.php?offset=0&limit=6000&filter=&orderby=&dir=1&partnerID=2" _
        , Destination:=Range("$A$1"))
